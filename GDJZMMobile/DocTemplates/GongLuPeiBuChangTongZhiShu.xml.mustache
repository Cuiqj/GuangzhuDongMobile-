        <pdf:text point="60,32" fontName="SimHei" fontSize="20.5">广 珠 东 公 路 赔 （补） 偿 通 知 书</pdf:text>
        
        <pdf:text point="72,68">（</pdf:text>
        <pdf:text point="76,68" static="NO">{{ caseMark2 }}</pdf:text>
        <pdf:text point="88,68">）年</pdf:text>
