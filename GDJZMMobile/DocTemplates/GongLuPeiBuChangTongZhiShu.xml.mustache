        <pdf:text point="113,68">交赔字第</pdf:text>
        <pdf:text point="134,68" maxWidth="20" static="NO">{{ caseMark3 }}</pdf:text>
        <pdf:text point="148,68">号</pdf:text>
        
        <pdf:text point="25,93">当  事  人:</pdf:text>
        <pdf:line fromPoint="56,98" toPoint="95,98" lineWidth="0.8"/>
        <pdf:text point="56,98" static="NO" fontSize="10">{{ partyName }}</pdf:text>
        
        <pdf:text point="25,103">当事人地址:</pdf:text>
        <pdf:line fromPoint="56,108" toPoint="160,108" lineWidth="0.8"/>
        <pdf:text point="56,109" maxWidth="90" static="NO" fontSize="10">{{ partyAddress }}</pdf:text>
        
        <pdf:text point="36,113">当事人</pdf:text>
        <pdf:line fromPoint="50,118" toPoint="102,118" lineWidth="0.8"/>
        <pdf:text point="53,120" maxWidth="94" static="NO" fontSize="10">{{ caseReason }}</pdf:text>
        <pdf:text point="103,113">一案，经</pdf:text>
        <pdf:line fromPoint="122,118" toPoint="152,118" lineWidth="0.8"/>
        <!--<pdf:text point="119,98" maxWidth="42" static="NO" fontSize="10">{{ agencyCity }}</pdf:text>
         <pdf:text point="119,102" maxWidth="36" static="NO" fontSize="10">{{ agency }}</pdf:text>-->
        <!--<pdf:text point="121,112" maxWidth="34" static="NO" fontSize="10">{{ agency }}</pdf:text>-->
